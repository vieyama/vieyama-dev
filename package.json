{
  "name": "fishfin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "lint": "next lint --fix",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "check:dead-ts": "ts-prune | grep -v '(used in module)'",
    "check:ts": "tsc",
    "check:lint": "next lint . --quiet --ignore-path .gitignore",
    "checker": "yarn check:dead-ts",
    "format:lint": "next lint . --fix --quiet --ignore-path .gitignore",
    "postinstall": "husky install",
    "commit": "cz",
    "prepare": "husky install",
    "push-ngrok": "ngrok http 3000",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "generate-icon-type": "node ./src/components/UI/Icon/iconType.js"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "eslint --fix",
      "yarn format"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@types/node": "20.4.9",
    "@types/react": "18.2.20",
    "@types/react-dom": "18.2.7",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "icomoon-react": "^3.0.0",
    "next": "13.4.13",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.0.0",
    "tailwind-merge": "^1.14.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@tanstack/react-query": "^4.0.0",
    "@tanstack/react-query-devtools": "^4.0.0",
    "@tanstack/react-query-next-experimental": "^5.0.0-alpha.80",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "autoprefixer": "10.4.14",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.46.0",
    "eslint-config-next": "^13.4.13",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": ">=7",
    "lint-staged": ">=10",
    "postcss": "8.4.27",
    "prettier": "^3.0.1",
    "prettier-plugin-tailwindcss": "^0.4.1",
    "tailwindcss": "3.3.3",
    "ts-pattern": "^5.0.5",
    "ts-prune": "^0.10.3",
    "typescript": "5.1.6"
  }
}
